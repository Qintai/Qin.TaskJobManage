		<EmbeddedResource Include="view\TaskJobUI2\assets\index-2c5ca1bf.js" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\index-b55cc30c.css" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\JobDetail-5ab068c4.js" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\JobDetail-9b42429b.css" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\JobTable-b9152869.css" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\JobTable-babc87bf.js" />
