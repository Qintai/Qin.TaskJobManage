<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<DisableImplicitNamespaceImports>true</DisableImplicitNamespaceImports>
		<Nullable>enable</Nullable>
		<Version>1.0.6.3</Version>
		<Authors>Qin</Authors>
		<PackageId>Qin.TaskJobManage</PackageId>
		<Company></Company>
		<PackageIcon>Qin.TaskJobManage_img.jpg</PackageIcon>
		<Description>基于Quartz的封装，提供UI管理界面(不兼容net framework)</Description>
		<PackageProjectUrl>https://www.cnblogs.com/Qintai/p/15738336.html</PackageProjectUrl>
		<RepositoryUrl></RepositoryUrl>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageReleaseNotes>优化中间件2</PackageReleaseNotes>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<EmbeddedResource Include="Qin.TaskJobManage_img.jpg">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</EmbeddedResource>
	</ItemGroup>
	
	<ItemGroup>
		<EmbeddedResource Include="view\StaticFileConfig.xml" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\el-input-3645df6a.js" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\el-input-a6832704.css" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\HelloWorld-622a7dd6.js" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\HelloWorld-9dbc4909.css" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\HttpApi-2e362149.js" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\index-b55cc30c.css" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\index-c267af4a.js" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\index-e1cc1afd.js" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\JobDetail-9b42429b.css" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\JobDetail-f0e6d8b5.js" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\JobTable-c3cd3d51.js" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\JobTable-d4e24ae5.css" />
		<EmbeddedResource Include="view\TaskJobUI2\assets\MyTable-0c2c5c71.js" />
		<EmbeddedResource Include="view\TaskJobUI2\favicon.ico" />
		<EmbeddedResource Include="view\TaskJobUI2\flexible.js" />
		<EmbeddedResource Include="view\TaskJobUI2\index.html" />
	</ItemGroup>
	
	<ItemGroup>
	  <None Include="..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Hosting" Version="2.2.7" />
		<PackageReference Include="Microsoft.AspNetCore.Hosting.Abstractions" Version="2.2.0" />
		<PackageReference Include="Microsoft.AspNetCore.Http" Version="2.2.2" />
		<PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
		<PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="2.2.0" />
		<PackageReference Include="Microsoft.Extensions.Configuration" Version="6.0.1" />
		<PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="6.0.0" />
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.1" />
		<PackageReference Include="Quartz" Version="3.7.0" />
	</ItemGroup>
	
</Project>
